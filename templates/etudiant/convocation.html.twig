{% extends 'base.html.twig' %}


{% block body %}
    

    <div class="card" style="width:400px;height:400px;margin-left:255px;">
        <div class="card-body">
            <div class="card" style="width:1200px;">
                <div class="card-body bs-card-subtitle-color">
                    <div id="alert-div"></div>
                    <div class="row">
          
                        {% for etudient in etudients %}
                        
                            <div class="col-md-4">
                                <div class="student-card">
                                    <h4>Nom:{{ etudient.nom }}</h4>
                                    <h4>Prénom:{{ etudient.prenom }}</h4>
                                    <h4>Convocation: {{ etudient.convocation }}</h4>
                                    <h4>Etablissement: {{ etudient.etablissement }}</h4>
                                    <h4>Série: {{ etudient.serie }}</h4>
                                    ----------------------------------------------------
                                    <p>convocation EPS</p>
                                     <h4>Convocation: {{ etudient.convocation }}</h4>
                                      <h4>{{ etudient.collective }}</h4>
                                      <h4>{{ etudient.collective }}</h4>

                                 
                                </div>
                            </div>
                        {% else %}
                            <div class="col-md-12">
                                <p>Aucun enregistrement trouvé</p>
                            </div>
                        {% endfor %}
                    </div>
                </div>
            </div>

   
       
{% endblock %}
