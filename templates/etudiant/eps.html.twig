{% extends 'base.html.twig' %}

{% block body %}
    <div class="card" style="width: 1200px;">
        <div class="card-body bs-card-subtitle-color">
            <div id="alert-div"></div>
            <div class="row">
                {% for etudient in etudiants %}
                    <div class="col-md-4">
                        <div class="student-card">
                          
                            <hr>
                            <p>Convocation EPS</p>
                             <h4>{{ etudient.nom }}</h4>
                            <h4>{{ etudient.prenom }}</h4>
                           
                        </div>
                    </div>
                {% else %}
                    <div class="col-md-12">
                        <p></p>
                    </div>
                {% endfor %}
            </div>
        </div>
    </div>
{% endblock %}
